{% extends 'base.html.twig' %}

{% block title %}Créer un article{% endblock %}

{% block body %}
    <h1>Créer un article</h1>
    {{ form_start(form) }}
        {{ form_row(form.title) }}
        {{ form_row(form.content) }}
        <button class="btn btn-success">Créer</button>
    {{ form_end(form) }}
    <a href="{{ path('article_list') }}">Retour à la liste</a>
{% endblock %}
